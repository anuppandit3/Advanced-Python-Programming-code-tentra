
class Car:
    def setenginemodel(self, engine):
        self.engine = engine

    def getenginemodel(self):
        print(self.engine)

class Tyre:
    def settyrenumber(self, num):
        self.num = num

    def gettyrenumber(self):
        print(self.num)

class Honda:
    def setcarmodel(self, model):
        self.model = model

    def getcarmodel(self):
        print(self.model)

# Create a new class that inherits from Car, Tyre, and Honda
class Accord(Car, Tyre, Honda):
    pass

# Now, create an instance of Accord
accord = Accord()

# Set values for engine, car model, and tyre number
accord.setenginemodel('EK-1')
accord.setcarmodel('V6')
accord.settyrenumber(236)

# Print car details
#print('Car Details: ')
accord.gettyrenumber()
